{
  "name": "masugadesign/labreports",
  "description": "Custom content/data reporting for Craft CMS.",
  "version": "2.0.3",
  "type": "craft-plugin",
  "minimum-stability": "dev",
  "require": {
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Masuga\\LabReports\\": "src/"
    }
  },
  "support": {
    "email": "info@gomasuga.com",
    "issues": "https://github.com/masugadesign/lab-reports-craft-cms/issues",
    "source": "https://github.com/masugadesign/lab-reports-craft-cms",
    "docs": "https://github.com/masugadesign/lab-reports-craft-cms/blob/master/README.md"
  },
  "extra": {
    "handle": "labreports",
    "name": "Lab Reports",
    "developer": "Masuga Design",
    "schemaVersion": "1.0.0",
    "developerUrl": "https://gomasuga.com",
    "changelogUrl": "https://raw.githubusercontent.com/masugadesign/lab-reports-craft-cms/master/CHANGELOG.md",
	"class": "Masuga\\LabReports\\LabReports"
  },
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true
    }
  }
}
